<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="400.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="devischantier.RootLayoutController">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu fx:id="exit" mnemonicParsing="false" text="Exit">
               <items>
                  <MenuItem fx:id="close" mnemonicParsing="false" onAction="#closeApplication" text="Close" />
               </items></Menu>
          <Menu fx:id="about" mnemonicParsing="false" text="About" />
        </menus>
      </MenuBar>
   </top>
</BorderPane>
